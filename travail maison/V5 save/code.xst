	<program>
		<identProgram name="ascenseur_code" type="section" task="MAST"></identProgram>
		<STSource>(* "moving" :
	0 : no move
	1 : up
	2 : down
	3: break*)

(*TO DO:
	- mettre en moving = 0  dans le cas d'une fin de course (haut/bas)
	- mettre en mode sécurité dans le cas d'appuis du bouton d'arret d'urgence (verifier la variable)*)

if not security_mode then

(* etage 1 *)
	if Ap_cab_1 then called_cab_1 := true;end_if;
	if Ap_mo_1 then called_mo_1 := true;  end_if;

	if called_stage_1 and allow_restart  then
		if moving = 0 then
			if last_floor &gt; 1 then 
				moving := 2;
			end_if;
		elsif moving = 3 then
			moving := moved;
		end_if;
	end_if;

	if Det_et_1 then 
		last_floor :=1;
		if called_stage_1 then 
			moving :=0;
			break:= true;
			called_cab_1 := false;
			called_mo_1 :=false;
			break:= false;
		end_if;
	end_if;


(* etage 2 *)
	if Ap_cab_2 then called_cab_2 := true;end_if;
	if Ap_mo_2 then called_mo_2 := true;  end_if;
	if Ap_2_de then called_de_2 := true;  end_if;


	if called_stage_2 and allow_restart then
		if moving = 0 then
			if last_floor &gt; 2 then 
				moving := 2;
			elsif last_floor &lt; 2 then
				moving := 1;
			end_if;
		elsif moving = 3 then
			moving := moved;
		end_if;
	end_if;

	if Det_et_2 then
		last_floor :=2;
		if called_stage_2 and not(called_stage_1 or called_stage_3 or called_stage_4 or called_stage_5)then
			moving :=0;
		elsif called_stage_2 then
			if not called_cab_2 then
				case moving of
				1: 	if called_mo_2 then 
						moved := moving;
					end_if;
				2:	if called_de_2 then 
						moved := moving;
						moving :=3 ;
					end_if;
				end_case;
			else
				moved := moving;
				moving :=3;

			end_if;
			
		end_if;
		if moving = 3 or moving = 0 then
			break := true;
			if moving = 3 then
				case moved of
					1:	if not(called_stage_3 or called_stage_4 or called_stage_5) then 
							moving :=0;
						end_if;
					2:	if not (called_stage_1) then 
							moving :=0;
						end_if;
				end_case;
			end_if;
			called_mo_2 := false;
			called_de_2 := false;
			called_cab_2 := false;
			break := false;
		end_if;
	end_if;

(* etage 3 *)
	if Ap_cab_3 then called_cab_3 := true;end_if;
	if Ap_mo_3 then called_mo_3 := true;  end_if;
	if Ap_3_de then called_de_3 := true;  end_if;


	if called_stage_3 and allow_restart then
		if moving = 0 then
			if last_floor &gt; 3 then 
				moving := 2;
			elsif last_floor &lt; 3 then
				moving := 1;
			end_if;
		elsif moving = 3 then
			moving := moved;
		end_if;
	end_if;

	if Det_et_3 then
		last_floor :=3;
		if called_stage_3 and not(called_stage_1 or called_stage_2 or called_stage_4 or called_stage_5)then
			moving :=0;
		elsif called_stage_3 then
			if not called_cab_3 then
				case moving of
				1: 	if called_mo_3 then 
						moved := moving;
						moving :=3 ; 
					end_if;
				2:	if called_de_3 then 
						moved := moving;
						moving :=3 ;
					end_if;
				end_case;
			else
				moved := moving;
				moving :=3;

			end_if;
			
		end_if;
		if moving = 3 or moving = 0 then
			break := true;
			if moving = 3 then
				case moved of
					1:	if not( called_stage_4 or called_stage_5) then 
							moving :=0;
						end_if;
					2:	if not (called_stage_1 or called_stage_2) then 
							moving :=0;
						end_if;
				end_case;
			end_if;
			called_mo_3 := false;
			called_de_3 := false;
			called_cab_3 := false;
			break := false;
		end_if;
	end_if;

(* etage 4 *)
	if Ap_cab_4 then called_cab_4 := true;end_if;
	if Ap_mo_4 then called_mo_4 := true;  end_if;
	if Ap_4_de then called_de_4 := true;  end_if;


	if called_stage_4 and allow_restart then
		if moving = 0 then
			if last_floor &gt; 4 then 
				moving := 2;
			elsif last_floor &lt; 4 then
				moving := 1;
			end_if;
		elsif moving = 3 then
			moving := moved;
		end_if;
	end_if;

	if Det_et_4 then
		last_floor :=2;
		if called_stage_4 and not(called_stage_1 or called_stage_2 or called_stage_3 or called_stage_5)then
			moving :=0;
		elsif called_stage_4 then
			if not called_cab_4 then
				case moving of
				1: 	if called_mo_4 then 
						moved := moving;
						moving :=3 ; 
					end_if;
				2:	if called_de_4 then 
						moved := moving;
						moving :=3 ;
					end_if;
				end_case;
			else
				moved := moving;
				moving :=3;
				
			end_if;
			
		end_if;
		if moving = 3 or moving = 0 then
			break := true;
			if moving = 3 then
				case moved of
					1:	if not( called_stage_5) then 
							moving :=0;
						end_if;
					2:	if not (called_stage_1 or called_stage_2 or called_stage_3 ) then 
							moving :=0;
						end_if;
				end_case;
			end_if;
			called_mo_4 := false;
			called_de_4 := false;
			called_cab_4 := false;
			break := false;
		end_if;
	end_if;

(* etage 5 *)
	if Ap_cab_5 then called_cab_5 := true;end_if;
	if Ap_5_de then called_de_5 := true;  end_if;


	if called_stage_5 and allow_restart then
		if moving = 0 then
			if last_floor &lt; 5 then
				moving := 1;
			end_if;
		elsif moving = 3 then
			moving := moved;
		end_if;
	end_if;

	if Det_et_5 then	
		last_floor :=5;
		if called_cab_5 then 
			break := true;
			moving :=0;
			called_cab_5 := false;
			break := false;
		end_if;
	end_if;
	if Fdc_bas (*or Fdc_haut  fin de course ou*) then moving := 0 ; Mont_cab:=false ;  Desc_cab:=false;end_if;
(*	if Cmd_r_s then security_mode := true; end_if; BOUTON ARRET D'URGENCE ACTIVE CMD_R_S?*)
	
	if not is_doors_closed then
		if moving =0 or moving = 3 then
			case last_floor of
				1: 	if ( Porte_et2 or Porte_et3 or Porte_et4 or Porte_et5) then security_mode := true ; end_if;
				2:	if ( Porte_et1 or Porte_et3 or Porte_et4 or Porte_et5) then security_mode := true ; end_if;
				3:	if ( Porte_et1 or Porte_et2 or Porte_et4 or Porte_et5) then security_mode := true ; end_if;
				4:	if ( Porte_et1 or Porte_et2 or Porte_et3 or Porte_et5) then security_mode := true ; end_if;
				5:	if ( Porte_et1 or Porte_et2 or Porte_et3 or Porte_et4) then security_mode := true ; end_if;
				else security_mode := true;
			end_case;

		else
			security_mode := true;
		end_if;		
	end_if;
	if security_mode then

		last_floor :=0;
		moving :=0;
		Mont_cab := false;
		Desc_cab := false;	
		
		called_cab_1:=false;
		called_cab_2:=false;
		called_cab_3:=false;
		called_cab_4:=false;
		called_cab_5:=false;
		
		called_de_2:=false;
		called_de_3:=false;
		called_de_4:=false;
		called_de_5:=false;

		called_mo_1:=false;
		called_mo_2:=false;
		called_mo_3:=false;
		called_mo_4:=false;
	end_if;

else 
	if not Cmd_r_s(*BOUTON ARRET D'URGENCE ACTIVE CMD_R_S?*) then
		if Ap_cab_1 then called_cab_1 := true;end_if;
		if Ap_mo_1 then called_mo_1 := true;  end_if;

		if called_stage_1 then
			moving := 2;
		end_if;

		if Det_et_1 then 
			last_floor :=1;
			moving :=0;
			called_cab_1 := false;
			called_mo_1 :=false;
		end_if;
	end_if;
	if not is_doors_closed or(*BOUTON ARRET D'URGENCE ACTIVE CMD_R_S?*) Cmd_r_s then

		last_floor :=0;
		moving :=0;

		Mont_cab := false;
		Desc_cab := false;
			
		called_cab_1:=false;
		called_mo_1:=false;		
	end_if;

end_if;

case moving of
0,3:	Mont_cab := false;
	Desc_cab := false;

1: 	Mont_cab := true; 
	Desc_cab := false; 
	moved := 1;

2: 	Mont_cab := false; 
	Desc_cab := true; 
	moved := 2;

end_case;

case last_floor of
	1:	AFFICHEUR[0] := TRUE;
		AFFICHEUR[1] := FALSE;
		AFFICHEUR[2] := FALSE;

	2:	AFFICHEUR[0] := FALSE;
		AFFICHEUR[1] := TRUE;
		AFFICHEUR[2] := False;

	3:	AFFICHEUR[0] := TRUE;
		AFFICHEUR[1] := TRUE;
		AFFICHEUR[2] := FALSE;

	4:	AFFICHEUR[0] := FALSE;
		AFFICHEUR[1] := FALSE;
		AFFICHEUR[2] := TRUE;

	5:	AFFICHEUR[0] := TRUE;
		AFFICHEUR[1] := FALSE;
		AFFICHEUR[2] := TRUE;
	else 
		AFFICHEUR[0] := FALSE;
		AFFICHEUR[1] := FALSE;
		AFFICHEUR[2] := FALSE;
end_case;
</STSource>
